class Main{
public static void main(String[] arg){
	int[][] check = new int[3][2];
    check[0][0] = 1;
    check[0][1] = 2;
	  check[1][0] = 3;
    check[1][1] = 4;
    check[2][0] = 5;
    check[2][1] = 6;
    for(int i=0;i<check.length;i++){
    	for(int j=0;j<check[i].length;j++){
        	System.out.print(check[i][j]+"  ");}
            System.out.println();
            }
        }
   }
