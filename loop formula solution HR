import java.util.*;
public class Main{
    public static void main(String[] ar){
        Scanner cam = new Scanner(System.in);
        int z = cam.nextInt();
        for(int i=0;i<z;i++){
            int y = cam.nextInt();
            int x = cam.nextInt();
            int w = cam.nextInt();
            for(int j=0;j<w;j++){
                y+=(Math.pow(2,j)*x);
                System.out.print(y+" ");
            }
        System.out.println();
        }
    }
}
