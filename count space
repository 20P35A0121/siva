import java.util.*;
class Main{
    public static void main(String[] args){
        int ans=0;
        Scanner fuck = new Scanner(System.in);
        System.out.println("Give me Input:");
        String s = fuck.nextLine();
        for(int i=0;i<s.length();i++){
            if(s.charAt(i) ==' '){
                ans++;
            }
        }
        System.out.println(ans+1);
    }
}
