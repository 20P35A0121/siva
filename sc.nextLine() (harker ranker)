import java.util.*;
public class Main{
    public static void main(String[] ar){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        double b=sc.nextDouble();
        sc.nextLine();
        String c=sc.nextLine();
        
        System.out.println("String :"+c);
        System.out.println("double :"+b);
        System.out.println("int :"+a);
    }
}
