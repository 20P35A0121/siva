class Main{
public static void main(String[] arg){
	int[][] check = {{1,2,1},{9,7,2},{7,6,4}};
    int[][] ans = new int[3][3];
    for(int i=0;i<check.length;i++){
    	for(int j=0;j<check[i].length;j++){
        		ans[i][j]=check[j][i];
            }
           }
           for(int i=0;i<ans.length;i++){
           	for(int j=0;j<ans[i].length;j++){
            	System.out.print(ans[i][j]+" ");}
                System.out.println();}
                }
               }
