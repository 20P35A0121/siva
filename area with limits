 import java.util.*;
class Main{
    public static void main(String[] ar){
        Scanner cam = new Scanner(System.in);
        int B = cam.nextInt();
        int H = cam.nextInt();
        if(B<=0 || H<=0){
            System.out.println("java.lang.Exception: Breadth and height must be positive");
        }
        else{
            if(B<=100 && H<=100){
                int A = B*H;
                System.out.println(A);
            }
        }
    }
}
