import java.util.*;
public class Main{
    public static void main(String[] ar){
        System.out.println("please enter your age: ");
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a>=18)
        {
            System.out.println("Adult");
        }
        else{
            System.out.println("not adult");
        }
        
    }
}
