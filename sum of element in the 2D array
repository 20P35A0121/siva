class Main{
public static void main(String[] arg){
	int[][] check = { {1,8,4},{9,7,2},{7,6,4}};
    int ans =0;
    for(int i=0;i<check.length;i++){
    	for(int j=0;j<check[i].length;j++){
        	ans=ans+check[i][j];}
            }
            System.out.println(ans);
        }
   }
   //ans = 48
